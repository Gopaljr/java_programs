package oops;

class Student{
	String name;
	int age;
	Student(String n,int a){
		 name=n;
		 age=a;
	}
	void display() {
		System.out.println("Name: "+name);
		System.out.println("Age: "+age);
	}
}

public class Interfaceprograms{
	public static void main(String[] args) {
		
		Student obj = new Student("Gopal",21);
		obj.display();
	}
}
