package Array;

public class ArrayExPrograms {

	public static void main(String[] args) {
		int[] arr= {10,15,20,25};
		int n=arr.length;
		
		int evenSum=0;
		int oddSum=0;
		
		for(int i=0;i<arr.length;i++) {
			int num = arr[i];
			
			if(num%2==0) {
				evenSum= evenSum+num;
			}
			else {
				oddSum = oddSum+num;
			}
		}
		System.out.println("Even Sum: "+evenSum);
		System.out.println("Odd Sum: "+oddSum);
	}
}
